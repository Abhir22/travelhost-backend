{
  "name": "travelhost-backend",
  "version": "1.0.0",
  "description": "Mifty Project",
  "main": "dist/servers/user.server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --require tsconfig-paths/register src/servers/user.server.ts",
    "dev:server": "ts-node-dev --respawn --require tsconfig-paths/register src/servers/user.server.ts",
    "dev:monitor": "mifty-monitor --auto-fix",
    "dev:db-designer": "cross-env DB_DESIGNER_PORT=3001 mifty-db-designer",
    "dev:full": "concurrently -n \"API,DB-Designer,Monitor,Prisma\" -c \"cyan,magenta,yellow,green\" \"npm run dev:server\" \"npm run dev:db-designer\" \"npm run dev:monitor\" \"npm run prisma:studio\"",
    "build": "rimraf dist && tsc && tsc-alias",
    "start": "node dist/servers/user.server.js",
    "generate": "mifty-generate",
    "generate:module": "mifty-generate generate-module",
    "generate:ai": "mifty-generate generate-ai",
    "adapter": "mifty-adapter",
    "adapter:list": "mifty-adapter list",
    "adapter:install": "mifty-adapter install",
    "adapter:installed": "mifty-adapter installed",
    "auth:list": "mifty-adapter auth:list",
    "auth:install": "mifty-adapter auth:install",
    "monitor": "mifty-monitor",
    "monitor:imports": "mifty-monitor --mode imports",
    "monitor:errors": "mifty-monitor --mode errors",
    "monitor:autofix": "mifty-monitor --auto-fix",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/prisma/schema.prisma",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "tsyringe": "^4.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "mifty": "file:E:\\miftyjs",
    "prettier": "^2.8.8",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "5.8.2"
  }
}
